<p id="notice"><%= notice %></p>

<h1>Flowchart Edits</h1>

<table>
  <thead>
    <tr>
      <th>Flowcharts</th>
      <th>Name</th>
      <th>Content</th>
      <th>Zoomlvl</th>
      <th>Approved</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @flowchart_edits.each do |flowchart_edit| %>
      <tr>
        <td><%= flowchart_edit.flowcharts %></td>
        <td><%= flowchart_edit.name %></td>
        <td><%= flowchart_edit.content %></td>
        <td><%= flowchart_edit.zoomlvl %></td>
        <td><%= flowchart_edit.approved %></td>
        <td><%= link_to 'Show', flowchart_edit %></td>
        <td><%= link_to 'Edit', edit_flowchart_edit_path(flowchart_edit) %></td>
        <td><%= link_to 'Destroy', flowchart_edit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Flowchart Edit', new_flowchart_edit_path %>
